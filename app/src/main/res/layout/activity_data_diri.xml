<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_mix2"
    android:orientation="vertical"
    tools:context=".DataDiri">
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/background_white">
            <ImageView
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:src="@drawable/pngegg"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Data Diri"
                android:textColor="@color/colorDarkOcean"
                android:textStyle="bold"
                android:textSize="30sp"/>
        </LinearLayout>
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tablelayoutid"
            android:layout_marginTop="90dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp">
            <TableRow
                android:id="@+id/rowlay1"
                android:background="#FFFF"
                android:padding="5dp">
                <TextView
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:text="Nama "
                    android:textColor="@color/colorDark"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:id="@+id/namaDtDiri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/colorDark"
                    android:layout_marginLeft="10dp"
                    android:text=""
                    android:fontFamily="sans-serif"/>

            </TableRow>
            <TableRow
                android:id="@+id/rowlay2"
                android:background="#FFFF"
                android:padding="5dp">
                <TextView
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:text="NIM "
                    android:textColor="@color/colorDark"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:id="@+id/NPMdatadiri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/colorDark"
                    android:text=""
                    android:fontFamily="sans-serif"/>

            </TableRow>
            <TableRow
                android:id="@+id/rowlay3"
                android:background="#FFFF"
                android:padding="5dp">
                <TextView
                    android:layout_width="47dp"
                    android:layout_height="wrap_content"
                    android:text="Alamat"
                    android:textColor="@color/colorDark"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:id="@+id/alamatdtDiri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/colorDark"
                    android:text=""
                    android:fontFamily="sans-serif"/>

            </TableRow>
            <TableRow
                android:id="@+id/rowlay4"
                android:background="#FFFF"
                android:padding="5dp">
                <TextView
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:text="Email"
                    android:textColor="@color/colorDark"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:id="@+id/emaildtDiri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/colorDark"
                    android:text=""
                    android:fontFamily="sans-serif"/>

            </TableRow>
            <TableRow
                android:id="@+id/rowlay7"
                android:background="#FFFF"
                android:padding="5dp">
                <TextView
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:text="No HP"
                    android:textColor="@color/colorDark"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:id="@+id/noHPDtdiri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/colorDark"
                    android:text=""
                    android:fontFamily="sans-serif"/>

            </TableRow>
            <TableRow
                android:id="@+id/rowlay6"
                android:background="#FFFF"
                android:padding="5dp">
                <TextView
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:text="NIK"
                    android:textColor="@color/colorDark"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:id="@+id/NIKDtdiri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/colorDark"
                    android:text=""
                    android:fontFamily="sans-serif"/>

            </TableRow>
            <TableRow
                android:id="@+id/rowlay8"
                android:background="#FFFF"
                android:padding="5dp">
                <TextView
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:text="Gender"
                    android:textColor="@color/colorDark"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:id="@+id/genderDtdiri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/colorDark"
                    android:text=""
                    android:fontFamily="sans-serif"/>

            </TableRow>
            <TableRow
                android:id="@+id/rowlay12"
                android:background="#FFFF"
                android:padding="5dp">
                <TextView
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:text="Agama"
                    android:textColor="@color/colorDark"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:id="@+id/agamaDtdiri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/colorDark"
                    android:text=""
                    android:fontFamily="sans-serif"/>

            </TableRow>
            <TableRow
                android:id="@+id/rowlay13"
                android:background="#FFFF"
                android:padding="5dp">
                <TextView
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:text="Prodi"
                    android:textColor="@color/colorDark"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:id="@+id/prodiDtdiri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/colorDark"
                    android:text=""
                    android:fontFamily="sans-serif"/>

            </TableRow>
            <TableRow
                android:id="@+id/rowlay14"
                android:background="#FFFF"
                android:padding="5dp">
                <TextView
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:text="Kelas"
                    android:textColor="@color/colorDark"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:id="@+id/kelasDtdiri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/colorDark"
                    android:text=""
                    android:fontFamily="sans-serif"/>

            </TableRow>
            <TableRow
                android:id="@+id/rowlay15"
                android:background="#FFFF"
                android:padding="5dp">
                <TextView
                    android:layout_width="45dp"
                    android:layout_height="wrap_content"
                    android:text="Angkatan"
                    android:layout_weight="0.1"
                    android:textColor="@color/colorDark"
                    android:fontFamily="sans-serif"/>
                <TextView
                    android:id="@+id/angkatanDtdiri"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/colorDark"
                    android:text=""
                    android:fontFamily="sans-serif"/>

            </TableRow>
        </TableLayout>
</RelativeLayout>
